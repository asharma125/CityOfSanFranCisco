    <fields>
        <fullName>FormattedDuration__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(DurationInMillisecs__c / 1000 &gt;= 3600, LPAD(TEXT(FLOOR(DurationInMillisecs__c / 1000 / 3600)), 2, &apos;0&apos;) &amp; &apos;:&apos;, &apos;&apos;) &amp; LPAD(TEXT(FLOOR(MOD(DurationInMillisecs__c / 1000, 3600) / 60)), 2, &apos;0&apos;) &amp; &apos;:&apos; &amp; LPAD(TEXT(FLOOR(MOD(DurationInMillisecs__c / 1000, 60))), 2, &apos;0&apos;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Duration</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
